{
  "EGI_2_0_INC": {
    "input_file_extension": "xlsx",
    "file_pattern": "^EGI_2_0_EMEA_INC_\\d{14}\\.xlsx$",
    "sheet_name": "Report 1",
    "column_type_map": {
      "r_revenue_egi_nrt_id": "string",
      "final_rtm": "string",
      "region": "string",
      "customer_po_number": "string",
      "eg_end_customer_name": "string",
      "sales_order_line_item_number": "string",
      "delivery_identifier": "string",
      "material_number": "string",
      "financial_close_calendar_year_month_code": "string",
      "include_exclude": "string",
      "order_create_calendar_date": "date",
      "order_type": "string",
      "profit_center_hierarchy_level_3_description": "string",
      "profit_center_hierarchy_level_4_description": "string",
      "business_area_code": "string",
      "sku_description": "string",
      "route_to_market": "string",
      "sales_order_completion_status_code": "string",
      "sales_order_identifier": "string",
      "sc_bklg_catg": "string",
      "ship_to_customer_sta": "string",
      "shipment_date": "date",
      "sold_to_customer_sta": "string",
      "source_type": "string",
      "sum_of_base_quantity": "decimal(30,15)",
      "sum_of_sales_order_base_quantity": "decimal(30,15)",
      "sum_of_cp_enterprise_standard_cost_(usd)": "decimal(30,15)",
      "sum_of_cp_factory_margin_(usd)": "decimal(30,15)",
      "sum_of_cp_gross_margin_(usd)": "decimal(30,15)",
      "sum_of_cp_gross_revenue_(usd)": "decimal(30,15)",
      "sum_of_cp_total_cost_of_sales_(usd)": "decimal(30,15)",
      "sum_of_cp_unit_quantity": "decimal(30,15)",
      "sum_of_gross_revenue_(usd)": "decimal(30,15)",
      "sum_of_net_revenue": "decimal(30,15)",
      "secured_position_net_usd": "decimal(30,15)",
      "sum_of_order_item_net_value_(usd)": "decimal(30,15)",
      "sum_of_order_item_net_value_document_currency": "decimal(30,15)",
      "process_timestamp": "timestamp"
    },
	"dq_checks": {
		  "trim_spaces": true,
		  "remove_special_characters": true,
		  "remove_alpha_from_numeric": false,
		  "row_duplicate_check": true,
		  "null_notnull_check": true,
		  "decimal_column_normalization": true
		},
    "column_mapping": {
      "sap_order_key": {
        "concat_ws": {
          "sep": "-",
          "columns": ["sales_order_identifier", "sales_order_line_item_number"]
        }
      },
      "region_id": { "lit": 500 },
      "fiscal_quarter": {
        "function": "fiscal_quarter_from_date",
        "args": ["order_create_calendar_date"]
      },
      "profit_center_hierarchy_level_3_description": { "lit": "WW" }
    },
    "primary_key_columns": [
		"order_type",
		"order_create_calendar_date",
		"sales_order_identifier",
		"sales_order_line_item_number",
		"source_type",
		"shipment_date",
		"delivery_identifier"
    ],
    "hash_columns": [
        "fiscal_quarter",
		"region",
		"region_id",
		"sap_order_key",
		"eg_end_customer_name",
		"final_rtm",
		"secured_position_net_usd",
		"sc_bklg_catg",
		"sku_description",
		"include_exclude",
		"business_area_code",
		"customer_po_number",
		"financial_close_calendar_year_month_code",
		"profit_center_hierarchy_level_3_description",
		"profit_center_hierarchy_level_4_description",
		"route_to_market",
		"sales_order_completion_status_code",
		"material_number",
		"sum_of_base_quantity",
		"sum_of_cp_unit_quantity",
		"sum_of_cp_enterprise_standard_cost_(usd)",
		"sum_of_cp_factory_margin_(usd)",
		"sum_of_cp_gross_margin_(usd)",
		"sum_of_cp_gross_revenue_(usd)",
		"sum_of_cp_total_cost_of_sales_(usd)",
		"sum_of_gross_revenue_(usd)",
		"sum_of_net_revenue",
		"sum_of_order_item_net_value_(usd)",
		"sold_to_customer_sta",
		"ship_to_customer_sta",
		"sum_of_sales_order_base_quantity",
		"sum_of_order_item_net_value_document_currency",
		"process_timestamp"
    ],
    "watermark_column": "process_timestamp"
  }
}
